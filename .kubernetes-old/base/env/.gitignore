# Terraform state files (may contain sensitive data)
*.tfstate
*.tfstate.*

# Crash log files
crash.log

# Override files (e.g., generated by terraform fmt)
*.tfvars
*.tfvars.json

# Terraform plan files (temporary execution plans)
*.tfplan

# Terraform module directory (downloaded modules and cache)
.terraform/
.terraform.lock.hcl

# Environment files (e.g., .env, may contain secrets)
.env
.terraformrc

# IDE/Editor directories (e.g., VS Code, IntelliJ)
.idea/
.vscode/

# Temporary/backup files
*.backup
*.bak